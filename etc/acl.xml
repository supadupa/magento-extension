<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
  <acl>
    <resources>
      <resource id="Magento_Backend::admin">
        <resource id="Magento_Backend::stores">
          <resource id="Magento_Backend::stores_settings">
            <resource id="Magento_Config::config" title="Configuration" sortOrder="50" />
            <resource id="Magento_Config::stores_all" title="Store Data" sortOrder="50" />
          </resource>
        </resource>
        <resource id="Magento_Catalog::catalog" >
          <resource id="Magento_Catalog::catalog_inventory" >
            <resource id="Magento_Catalog::products" >
              <resource id="AddSauce_App::products" title="AddSauce Feed" sortOrder="100" />
            </resource>
          </resource>
        </resource>
      </resource>
    </resources>
  </acl>
</config>

